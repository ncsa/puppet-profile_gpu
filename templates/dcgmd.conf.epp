<%- | $dcgm_telegraf_port,
| -%>
# This file is managed by Puppet, manual changes will be lost
[[inputs.socket_listener]]
  name_override = "dcgm"
  service_address = "udp://localhost:<%= $dcgm_telegraf_port %>"
  data_format = "json"
  tag_keys = [
    "compute_pids",
    "driver_version",
    "gpu_uuid",
    "nvml_version",
    "process_name",
    "xid_errors"
  ]
